{% extends 'base.html.twig' %}
{% block stylesheets %}
	<link rel="stylesheet" href={{asset('css/bootstrap.min.css')}}>
	<link rel="stylesheet" href={{asset('css/style.css')}}>
{% endblock %}
{% block body %}

<i><center><h1 class="text-muted">Email ou mot de passe incorrect</h1></center></i>
<div class='formulaire'>
	
	<div class="container">
		<div class="row">
			<div class="col-sm-4">
				
			</div>
			<div class="form-group col-sm-4">
	{{ form_start(form) }}
		{{ form_row(form.Email, {'attr': {'placeholder': "Votre Email..",'class':'form-control','style':'100px'}}) }}
		{{ form_row(form.mot_de_passe, {'attr': {'placeholder': "Votre Mot De Passe..",'class':'form-control'}}) }}
		{{ form_row(form.save, {'attr': {'class':'btn btn-primary'}}) }}
	{{ form_end(form) }}
	</div>
	<div class="col-sm-4">
	</div>
		
	</div>
	
</div>

{% endblock %}